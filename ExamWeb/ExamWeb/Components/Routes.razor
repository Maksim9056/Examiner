@using ExamWeb.Components.Pages
@inject Home Homes

<Router AppAssembly="typeof(Program).Assembly" AdditionalAssemblies="new[] { typeof(ExamWeb.Client._Imports).Assembly }">
    <Found Context="routeData">
        <RouteView RouteData="routeData" DefaultLayout="typeof(Layout.MainLayout)" />
        <FocusOnNavigate RouteData="routeData" Selector="h1" />
    </Found>
</Router>

@code {
    [Parameter]
    public string UrlAddress { get; set; } = "Привет";

    protected override async Task OnInitializedAsync()
    {
        Homes.urlAddres = UrlAddress;

    }
}